{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base/legacy", "eslint:recommended", "plugin:vue/essential", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "plugins": ["vue"],
  "rules": {
    "no-shadow": "off",
    "no-param-reassign": ["off"],
    "func-names": "off"
  }
}
