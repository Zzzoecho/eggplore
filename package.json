{
  "name": "egg-plore",
  "version": "0.0.1-beta",
  "description": "eggplore UI",
  "main": "lib/index.umd.js",
  "unpkg": "lib/index.min.js",
  "files": [
    "packages",
    "src",
    "lib"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zzzoecho/eggplore.git"
  },
  "prepublishOnly": "npm run build",
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "scripts": {
    "dev": "webpack-dev-server --watch --config webpack.config.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "clean": "rimraf lib",
    "build": "npm run clean & npm run lint & npm run build:umd & npm run build:unpkg & npm run build:es",
    "build:umd": "rollup -c --format umd --file lib/index.umd.js",
    "build:es": "rollup -c --format es --file lib/index.esm.js",
    "build:unpkg": "rollup -c --format iife --file lib/index.min.js",
    "lint": "eslint src/**/* packages/**/**/* --quiet"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-image": "^2.0.6",
    "@types/eslint-config-prettier": "6.11.0",
    "@vue/compiler-sfc": "^3.0.7",
    "@vuepress/plugin-register-components": "^1.7.1",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.2.8",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "5.1.9",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "vue-loader": "^15.9.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.7.1",
    "vuepress-plugin-demo-container": "^0.2.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "cache-loader": "^4.1.0",
    "element-ui": "^2.15.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9"
  }
}
